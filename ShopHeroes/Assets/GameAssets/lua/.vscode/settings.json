{
    "Lua.runtime.path": [
        "?.lua",
        "?/init.lua",
        "?/?.lua",
        "?.lua.txt",
        "?/?.lua.txt"
    ],
    "files.associations": {
        "*.lua" : "lua",
        "*.txt" : "lua"
    },
    "files.exclude":{
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.meta" : true
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#317c2e",
        "activityBar.activeBorder": "#a2a4dc",
        "activityBar.background": "#317c2e",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#a2a4dc",
        "activityBarBadge.foreground": "#15202b",
        "statusBar.background": "#225720",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#317c2e",
        "titleBar.activeBackground": "#225720",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#22572099",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#225720",
    "Lua.diagnostics.globals": [
        "inst"
    ]
}